server {
  listen 80;

  server_name docker_example.local;
  client_body_buffer_size     10M;
  client_max_body_size        10M;


  location / {
    proxy_pass http://frontend:4200;
  }

  location /api {
    proxy_pass http://api:3001;
    rewrite ^/api/(.*) /$1 break;
     client_body_buffer_size     10M;
     client_max_body_size        10M;
  }

}